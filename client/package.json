{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "@stitches/react": "^1.2.8",
    "babylonjs": "^4.2.0",
    "babylonjs-loaders": "^4.2.0",
    "cannon": "^0.6.2",
    "http-proxy-middleware": "^2.0.6",
    "jwt-decode": "^3.1.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-linkify": "^1.0.0-alpha",
    "react-popper-tooltip": "4.4.2",
    "react-redux": "^8.0.1",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^8.1.1",
    "react-transition-group": "^4.4.2",
    "redux": "^4.2.0",
    "redux-first-history": "^5.1.1",
    "regenerator-runtime": "^0.13.9",
    "reselect": "^4.1.5",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "start": "VITE_LOCAL_AUTH=true vite --host",
    "with-ssl": "VITE_LOCAL_AUTH=true USE_SSL_HMR=true vite --host",
    "with-ssl-no-auth": "USE_SSL_HMR=true vite --host",
    "no-auth": "vite --host",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "docs": "typedoc --plugin typedoc-plugin-markdown src/types/server_messages.ts src/types/store_types.ts --out ../docs/gen --publicPath '/gen/' && rm ../docs/gen/README.md && sed -i 's@\\[client\\](/gen/README.md)@\\[home\\](/)@' $(find ../docs/gen -type f)"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "import/no-anonymous-default-export": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/cannon": "^0.1.5",
    "@types/jest": "24.0.18",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "12.7.5",
    "@types/react": "17.0.2",
    "@types/react-dom": "^18.0.4",
    "@types/react-linkify": "^1.0.1",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-transition-group": "^4.4.4",
    "@types/reselect": "^2.2.0",
    "@types/uuid": "^8.3.1",
    "@vitejs/plugin-react": "^1.3.2",
    "prettier": "^2.8.4",
    "typedoc": "^0.23.26",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "4.9.5",
    "vite": "^2.9.13",
    "vite-aliases": "^0.9.2",
    "vite-plugin-svgr": "^2.1.0",
    "vite-tsconfig-paths": "^3.5.0"
  }
}
